/* Tall habitat item */
item (FEAT_HOUSES, item_tall_habitat) {
  property {
    substitute: 0;
    name: string(STR_NAME_TALL_HABITAT);
    probability: 15;
    years_available: [0, -1];
    minimum_lifetime: 0;
    population: 64;
    mail_multiplier: 24;
    availability_mask: [bitmask(TOWNZONE_INNER_SUBURB, TOWNZONE_CENTRE), ALL_CLIMATES | bitmask(ABOVE_SNOWLINE)];
    building_class: 1; // Walkway levels.
 accepted_cargos: [[PASS, 8], [FOOD, 6], [GOOD, 2]];
 removal_cost_multiplier: 150;
 local_authority_impact: 150;
  }

  graphics { default: random_tall_habitat; }
}
