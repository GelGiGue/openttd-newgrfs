/* NewGRF definition. */
grf {
  grfid: "OGM\5";
  name: string(STR_GRF_NAME);
  desc: string(STR_GRF_DESC);
  url: string(STR_GRF_URL);
  version: 5443;
  min_compatible_version: 1;
}



/* Disable all default houses. */
disable_item(FEAT_HOUSES);



/* Make heavily inhabited areas have pavement, streetlights even in desert. */
desert_paved_roads = 1;
