    sprite: spriteset_passageway_nw;
    xoffset: 6;
    yoffset: -8;
    zoffset: 23;
    xextent: 4;
    yextent: 8;
    zextent: 8;
    hide_sprite:
      nearby_tile_house_class(0,-1) > 256 ||
      nearby_tile_house_class(0,-2) < 257 ||
      construction_state < 3;
  }
  building {
    sprite: spriteset_passageway_ne;
