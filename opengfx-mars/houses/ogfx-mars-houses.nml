/* Tower habitat item. */
item (FEAT_HOUSES, item_tower_habitat) {
  property {
    substitute: 0;
    name: string(STR_NAME_TOWER_HABITAT);
    probability: 15;
    years_available: [0, -1];
    minimum_lifetime: 0;
    population: 128;
    mail_multiplier: 48;
    availability_mask: [bitmask(TOWNZONE_CENTRE), ALL_CLIMATES | bitmask(ABOVE_SNOWLINE)];
    building_class: 2; // Walkway levels.
 accepted_cargos: [[PASS, 8], [FOOD, 8], [GOOD, 4]];
 removal_cost_multiplier: 180;
 local_authority_impact: 180;
  }

  graphics { default: random_tower_habitat; }
}
