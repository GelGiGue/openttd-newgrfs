/* Templates. */
template template_building(n) {
  // left top width height x y
  [4 + 64 * n + 4 * n, 140, 64, 64, -32, -32]
  [4 + 64 * n + 4 * n, 72, 64, 64, -32, -32]
  [4 + 64 * n + 4 * n, 4, 64, 64, -32, -32]
}

template template_tall_building(n) {
  // left top width height x y
  [4 + 72 * n + 4 * n, 400, 72, 128, -36, -96]
  [4 + 72 * n + 4 * n, 268, 72, 128, -36, -96]
  [4 + 72 * n + 4 * n, 136, 72, 128, -36, -96]
  [4 + 72 * n + 4 * n, 4, 72, 128, -36, -96]
}
